<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN" "http://www.ibatis.com/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
<settings
	cacheModelsEnabled="true"
	enhancementEnabled="true"
	lazyLoadingEnabled="false"
	maxRequests="64"
	maxSessions="20"
	maxTransactions="8"
	useStatementNamespaces="false"
	classInfoCacheEnabled="true"
	errorTracingEnabled="true"
	statementCachingEnabled="true"
/>
    <sqlMap resource="org/opennms/acl/repository/ibatis/sqlmap/authority.xml"/>
    <sqlMap resource="org/opennms/acl/repository/ibatis/sqlmap/users.xml"/>
    <sqlMap resource="org/opennms/acl/repository/ibatis/sqlmap/groups.xml"/>
</sqlMapConfig>